<p id="notice"><%= notice %></p>

<h1>Ests</h1>

<table>
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Carrera</th>
      <th>Carnet</th>
      <th>Nacimiento</th>
      <th>Celular</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ests.each do |est| %>
      <tr>
        <td><%= est.nombres %></td>
        <td><%= est.apellidos %></td>
        <td><%= est.carrera %></td>
        <td><%= est.carnet %></td>
        <td><%= est.nacimiento %></td>
        <td><%= est.celular %></td>
        <td><%= link_to 'Show', est %></td>
        <td><%= link_to 'Edit', edit_est_path(est) %></td>
        <td><%= link_to 'Destroy', est, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Est', new_est_path %>
